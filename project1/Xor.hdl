/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    And(a=a, b=b, out=aandb);
    Or(a=a, b=b, out=aorb);
    Not(in=aorb, out=w1);
    Or(a=w1, b=aandb, out=w2);
    Not(in=w2, out=out);
    
    
    
    
    
}